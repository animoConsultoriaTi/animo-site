<section id="partners">
    <div class="container">
      <h2>Nossos Parceiros</h2>
      <div class="glide">
        <div class="glide__track" data-glide-el="track">
          <ul class="glide__slides">
            <% partners.forEach(({ attributes: { url, logo, name } })=> { %>
                <li class="glide__slide">
                  <a href="<%=url %>" class="slide-item" target="_blank">
                    <img src="<%=logo %>" alt="Parceiro - <%=name %>">
                  </a>
                </li>
            <% }) %>
          </ul>
        </div>
        <div class="glide__bullets" data-glide-el="controls[nav]"></div>
      </div>
  </section>