// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Service {
  id          String @id @default(uuid())
  slug        String @unique
  icon        String
  title       String
  description String
  methodology String
  help        String

  @@map("services")
}
